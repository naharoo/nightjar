<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>nexus-snapshots</id>
            <username>${env.MAVEN_ACCESS_USR}</username>
            <password>${env.MAVEN_ACCESS_PSW}</password>
        </server>

        <server>
            <id>nexus</id>
            <username>${env.MAVEN_ACCESS_USR}</username>
            <password>${env.MAVEN_ACCESS_PSW}</password>
        </server>

        <server>
            <id>docker-registry.dev.navads.eu</id>
            <username>${env.DOCKER_ACCESS_USR}</username>
            <password>${env.DOCKER_ACCESS_PSW}</password>
        </server>
    </servers>
</settings>
